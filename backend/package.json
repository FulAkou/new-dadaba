    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.3",
    "zod": "^3.24.2",
    "drizzle-orm": "^0.36.1",
    "pg": "^8.12.0"
  },
